<Window x:Name="RenderingWindow" x:Class="ProGenTracer.RenderWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProGenTracer"
        mc:Ignorable="d"
        Title="Render" Height="128" Width="128">
    <Grid>
        <Image x:Name="RenderImage" Source="{Binding ImageSource}" HorizontalAlignment="Left" Height="249" Margin="10,10,0,0" VerticalAlignment="Top" Width="272"/>

    </Grid>
</Window>
